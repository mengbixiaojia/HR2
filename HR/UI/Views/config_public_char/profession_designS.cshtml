
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>profession_designS</title>
    <link href="~/css/table.css" rel="stylesheet" />
    <script src="~/Scripts/comm/comm.js"></script>
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/vue.js"></script>
</head>
<body>
    <div id="div1"> 
        <form method="post" action="">
            <table width="100%">
                <tr>
                    <td>
                        <font color="black">
                            您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--职称设置
                        </font>
                    </td>
                </tr>
                <tr>
                    <td align="right"></td>
                </tr>
            </table>
            <table width="100%" border="1" cellpadding=0 cellspacing=1
                   bordercolorlight=#848284 bordercolordark=#eeeeee
                   class="TABLE_STYLE1">
                <tr>
                    <td width="80%" class="TD_STYLE1">
                        职称名称
                    </td>
                    <td width="20%" class="TD_STYLE1" align="center">
                        删除
                    </td>
                </tr>
                <tr v-for="(item,index) in dt">
                    <td class="TD_STYLE2">{{item.attribute_name}}</td>
                    <td class="TD_STYLE2"><a v-bind:href="['/config_public_char/Delete/'+item.Id]" onclick="return confirm('是否确定删除')">删除</a></td>
                </tr>
            </table>
        </form>
    </div>
    <script type="text/javascript">
        var vm = new Vue({
            el: "#div1",
            data: {
                dt:null
            },
            methods: {
                load: function () {
                    var t = this;
                    $.ajax({
                        url: "/config_public_char/Index3",
                        type: "post",
                        data: "",
                        success: function (msg) {
                            var dd = eval("(" + msg + ")");
                            t.dt = dd;
                        }
                    })
                }
            },
            created: function () {
                this.load();
            }
        })
    </script>
</body>
</html>
