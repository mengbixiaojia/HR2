
@{
    Layout = null;
}

<!DOCTYPE html>
@model Model.config_public_charModel
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Add</title>
    <link href="~/css/table.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.3.1.js"></script>

</head>
<body>
  @using (Ajax.BeginForm("Add", new AjaxOptions() { HttpMethod = "Post", OnSuccess = "show" }))
  {
        <table width="100%">
            <tr>
                <td>
                    <font color="black">
                        您正在做的业务是：人力资源--客户化设置--薪酬管理设这--薪酬项目设置--薪酬项目添加
                    </font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    <input type="submit" value="提交" class="BUTTON_STYLE1">
                    <input type="button" value="返回" class="BUTTON_STYLE1"
                           onclick="history.back();">
                </td>
            </tr>
        </table>

        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <tr>
                <td class="TD_STYLE1" width="19%">
                    薪酬项目名称
                </td>
                <td class="TD_STYLE2" width="81%">
                 @Html.TextBoxFor(model => Model.attribute_name, new { @class = "INPUT_STYLE1" })
                </td>
            </tr>
        </table>
  }
</body>
</html>
