
@{
    Layout = null;
}
@model Model.config_file_first_kindModel
<!DOCTYPE html>
<link href="~/css/table.css" rel="stylesheet" />
<script src="~/Scripts/comm/comm.js"></script>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>first_kind_change</title>
</head>
<body>
    <div> 
@using (Ajax.BeginForm("first_kind_change", new AjaxOptions() { HttpMethod = "post" }))
{
        <form name="configfilefirstkindForm" method="post"
              action="">
            <table width="100%">
                <tr>
                    <td>
                        <font color="black">
                            您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--I级机构设置--I级机构变更
                        </font>
                    </td>
                </tr>
                <tr>
                    <td align="right">
                        <input id="Submit1" type="submit" value="提交" class="BUTTON_STYLE1" />
                        <input id="Submit1" type="button" value="返回" class="BUTTON_STYLE1" />
                    </td>
                </tr>
            </table>

            <table width="100%" border="1" cellpadding=0 cellspacing=1
                   bordercolorlight=#848284 bordercolordark=#eeeeee
                   class="TABLE_STYLE1">
                <input type="hidden" name="cffk.ffkId" value="755" />
                <tr>
                    <td width="19%" class="TD_STYLE1">
                        I级机构编号
                    </td>
                    <td width="81%" class="TD_STYLE2">
                        @Html.TextBox("first_kind_id",Model.first_kind_id, new { @class="INPUT_STYLE1",@readonly="readonly"})
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        I级机构名称
                    </td>
                    <td class="TD_STYLE2">
                       @Html.TextBox("first_kind_name", Model.first_kind_name, new { @class = "INPUT_STYLE1", @readonly = "readonly" })
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        薪酬发放责任人编号（多个编号之间请用"半角逗号"加"一个空格"隔开，如", "）
                    </td>
                    <td class="TD_STYLE2">
                        @Html.TextArea("first_kind_salary_id", Model.first_kind_salary_id, new { @class = "TEXTAREA_STYLE1", rows = "4" })
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        销售责任人编号（多个编号之间请用"半角逗号"加"一个空格"隔开，如", "）
                    </td>
                    <td class="TD_STYLE2">
                        @Html.TextArea("first_kind_sale_id", Model.first_kind_sale_id, new { @class = "TEXTAREA_STYLE1", rows = "4" })
                    </td>
                </tr>
            </table>
        </form>
}
    </div>
</body>
</html>
