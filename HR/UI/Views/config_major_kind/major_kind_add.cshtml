
@{
    Layout = null;
}
@model Model.config_major_kindModel
<!DOCTYPE html>
<link href="~/css/table.css" rel="stylesheet" />
<script src="~/Scripts/comm/comm.js"></script>
<script src="~/Scripts/jquery-1.10.2.js"></script>
<script src="~/Scripts/jquery.validate.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>major_kind_add</title>
</head>
<body>
    <div> 
@using (Ajax.BeginForm("major_kind_add", new AjaxOptions() { HttpMethod = "post" }))
{

        <form action="major_kind.html" method="post">
            <table width="100%">
                <tr>
                    <td>
                        <font color="black">
                            您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--职位分类设置--职位分类添加
                        </font>
                    </td>
                </tr>
                <tr>
                    <td>
                        <input id="Submit1" type="submit" value="提交" class="BUTTON_STYLE1" />
                        <input id="Submit1" type="button" value="返回" class="BUTTON_STYLE1" />
                    </td>
                        </tr>
            </table>

            <table width="100%" border="1" cellpadding=0 cellspacing=1
                   bordercolorlight=#848284 bordercolordark=#eeeeee
                   class="TABLE_STYLE1">
                <tr>
                    <td class="TD_STYLE1" width="19%">
                        职位分类编号
                    </td>
                    <td width="81%" class="TD_STYLE2">@Html.TextBox("major_kind_id", Model.major_kind_id, new { @class = "INPUT_STYLE1" })</td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        职位分类名称
                    </td>
                    <td class="TD_STYLE2">
                        @Html.TextBox("major_kind_name", Model.major_kind_name, new { @class = "INPUT_STYLE1" })
                        @Html.ValidationMessageFor(e => e.major_kind_name)
                    </td>
                </tr>
            </table>
        </form>

}
    </div>
</body>
</html>
