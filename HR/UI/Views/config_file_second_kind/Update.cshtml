
@{
    Layout = null;
}

<!DOCTYPE html>
@using UI.Common
@model Model.config_file_second_kindModel
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Update</title>
    <script src="~/Scripts/jquery-3.3.1.js"></script>
    <link href="~/css/table.css" rel="stylesheet" />
    <script type="text/javascript">
        function show(msg) {
            alert(msg);
            if (msg == "OK") {
                alert('修改成功!');
                window.location.href = "../config_file_second_kind/Index";
            }
        }

    </script>
</head>
<body>

   @using (Ajax.BeginForm("Update", new AjaxOptions() { HttpMethod = "Post", OnSuccess = "show" }))
   {

        <table width="100%">
            <tr>
                <td>
                    <font color="black">
                        您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--II级机构设置--II级机构变更
                    </font>
                </td>
            </tr>
            <tr>
                <td align="right">
                  
                    @Html.HtmlSubmit();
                    <input type="button" value="返回" class="BUTTON_STYLE1"
                           onclick="history.back();">
                </td>
            </tr>
        </table>

        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <input type="hidden" name="cfsk.fskId" value=761 class="INPUT_STYLE1" />
            <tr>
                <td width="19%" class="TD_STYLE1">
                    I级机构名称
                </td>
                <td width="81%" class="TD_STYLE2">
                    @Html.TextBox("first_kind_name", Model.first_kind_name, new { @class = "INPUT_STYLE1", @readonly = "readonly" })
                </td>
            </tr>
            <tr>
                <td width="19%" class="TD_STYLE1">
                    II级机构编号
                </td>
                <td width="81%" class="TD_STYLE2">
                   
                    @Html.TextBox("second_kind_id", Model.second_kind_id, new { @class = "INPUT_STYLE1", @readonly = "readonly" })
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    II级机构名称
                </td>
                <td class="TD_STYLE2">
                   
                    @Html.TextBox("second_kind_name", Model.second_kind_name, new { @class = "INPUT_STYLE1", @readonly = "readonly" })
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    薪酬发放责任人编号（多个编号之间请用"半角逗号"加"一个空格"隔开，如", "）
                </td>
                <td class="TD_STYLE2">
                   
                    @Html.TextArea("second_salary_id", Model.second_salary_id, new { @class = "INPUT_STYLE1", @rows = "4", @cols = "120" })
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    销售责任人编号（多个编号之间请用"半角逗号"加"一个空格"隔开，如", "）
                </td>
                <td class="TD_STYLE2">
                 
                    @Html.TextArea("second_sale_id", Model.second_sale_id, new { @class = "INPUT_STYLE1", @rows = "4", @cols = "120" })
                </td>
            </tr>
        </table>
   }
</body>
</html>
